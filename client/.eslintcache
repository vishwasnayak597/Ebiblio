     <Link to={`/product/${slug}`}>\r\n          <EyeOutlined className=\"text-warning\" /> <br /> View Product\r\n        </Link>,\r\n        <Tooltip  title={tooltip}>\r\n          <a onClick={handleAddToCart} disabled={product.quantity<1}>\r\n          <ShoppingCartOutlined className=\"text-danger\" /> <br />\r\n            {product.quantity <1 ? 'Out of Stock':'Add to cart'}\r\n        </a>,\r\n        </Tooltip>\r\n        \r\n      ]}\r\n    >\r\n      <Meta\r\n        title={`${title}- Rs. ${price} `}\r\n        description={`${description && description.substring(0, 40)}...`}\r\n      />\r\n    </Card>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ProductCard;\r\n","C:\\web development\\ebiblio\\client\\src\\functions\\user.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\form\\fileUploader.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\form\\catagoryForm.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\form\\star.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\form\\productUpdateForm.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\form\\localsearch.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\cards\\adminProduct.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\routes\\LoadingToRedirect.js",[],"C:\\web development\\ebiblio\\client\\src\\functions\\coupon.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\order\\orders.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\form\\search.js",[],"C:\\web development\\ebiblio\\client\\src\\functions\\product.js",[],"C:\\web development\\ebiblio\\client\\src\\functions\\admin.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\order\\Invoice.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\cards\\ProductCardInCheckout.js",["340","341"],"import React from \"react\";\r\nimport ModalImage from 'react-modal-image';\r\nimport Book1 from '../../images/book1.jpg';\r\nimport { useDispatch } from \"react-redux\";\r\nimport { toast } from \"react-toastify\";\r\nimport {CheckCircleOutlined,CloseCircleOutlined,CloseOutlined} from \"@ant-design/icons\";\r\n\r\nconst ProductCardInCheckout = ({ p }) => {\r\n    let dispatch = useDispatch();\r\n\r\n    const handleQuantityChange = (e) => {\r\n    let count = e.target.value < 1 ? 1 : e.target.value;  //preventing qty less than 1\r\n\r\n    if (count > p.quantity) {     //check whether count is more than actual quantity in stock\r\n      toast.error(`Max available quantity: ${p.quantity}`);\r\n      return;\r\n    }\r\n\r\n    let cart = [];\r\n\r\n    if (typeof window !== \"undefined\") {  //if we have window obj grab local storage\r\n      if (localStorage.getItem(\"cart\")) {\r\n        cart = JSON.parse(localStorage.getItem(\"cart\"));\r\n      }\r\n\r\n      cart.map((product, i) => {\r\n        if (product._id === p._id) {  //check if product id matches one which is in card\r\n          cart[i].count = count;  //get each product\r\n        }\r\n      });\r\n\r\n      localStorage.setItem(\"cart\", JSON.stringify(cart)); //setback cart info to local storage\r\n      dispatch({\r\n        type: \"ADD_TO_CART\",\r\n        payload: cart,\r\n      });\r\n    }\r\n  };\r\n\r\n  const handleRemove = () => {\r\n    let cart = [];\r\n\r\n    if (typeof window !== \"undefined\") {   //checking if its there in local storage\r\n      if (localStorage.getItem(\"cart\")) {\r\n        cart = JSON.parse(localStorage.getItem(\"cart\"));\r\n      }\r\n      // [1,2,3,4,5]  suppose index 3 - 3rd item in array is removed then check and remove item\r\n      cart.map((product, i) => {\r\n        if (product._id === p._id) {\r\n          cart.splice(i, 1);  //remove item when product of actual item(p) matches of that in cart product\r\n        }\r\n      });\r\n\r\n      localStorage.setItem(\"cart\", JSON.stringify(cart));\r\n      dispatch({\r\n        type: \"ADD_TO_CART\",\r\n        payload: cart,\r\n      });\r\n    }\r\n  };\r\n\r\n  return (\r\n    <tbody>\r\n      <tr>\r\n        <td>\r\n          <div style={{width:\"100px\",height:\"auto\"}}>                             \r\n            {p.images.length ? (<ModalImage small={p.images[0].url} large={p.images[0].url} />)\r\n            :\r\n            (\r\n                <ModalImage small={Book1} large={Book1} />\r\n            ) }\r\n          </div>\r\n        </td>\r\n        <td>{p.title}</td>\r\n        <td>Rs. {p.price} </td>\r\n        <td>\r\n          <input\r\n            type=\"number\" step=\"1\"\r\n            className=\"form-control\"\r\n            value={p.count}\r\n            onChange={handleQuantityChange}\r\n          />\r\n        </td>\r\n        <td className=\"text-center\">\r\n          {p.shipping === \"Yes\" ? (\r\n            <CheckCircleOutlined className=\"text-success\" /> /* shipping icon */\r\n          ) : (\r\n            <CloseCircleOutlined className=\"text-danger\" />\r\n          )}\r\n        </td>\r\n        \r\n        <td className=\"text-center\">\r\n          <CloseOutlined\r\n            onClick={handleRemove}\r\n            className=\"text-danger pointer\"\r\n          />\r\n        </td>\r\n      </tr>\r\n    </tbody>\r\n  );\r\n};\r\n\r\nexport default ProductCardInCheckout;\r\n","C:\\web development\\ebiblio\\client\\src\\components\\cards\\ShowPaymentInfo.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\form\\productCreateForm.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\cards\\jumbotron.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\home\\newarrival.js",["342"],"import React, { useEffect, useState } from \"react\";\r\nimport { getProducts,getProductsCount} from \"../../functions/product\";\r\nimport ProductCard from \"../cards/productcard\";\r\nimport LoadingCard from \"../cards/loadingcard\";\r\nimport { Pagination } from \"antd\";\r\n\r\nconst NewArrivals = () => {\r\n  const [products, setProducts] = useState([]);\r\n  const [loading, setLoading] = useState(false);\r\n  const [productsCount, setProductsCount] = useState(0);\r\n  const [page, setPage] = useState(1);\r\n\r\n  useEffect(() => {\r\n    loadAllProducts();\r\n  }, [page]);  //component should run when page value is chnaged hence we have to give dependency \r\n\r\n  useEffect(() => {\r\n    getProductsCount().then((res) => setProductsCount(res.data));\r\n  }, []);\r\n\r\n  const loadAllProducts = () => {\r\n    setLoading(true);\r\n    //sort,order and limit based on page send those parans\r\n    getProducts(\"createdAt\", \"desc\", page).then((res) => {\r\n      setProducts(res.data);\r\n      setLoading(false);\r\n    });\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <div className=\"container\">\r\n        {loading ? (\r\n          <LoadingCard count={3} />\r\n        ) : (\r\n          <div className=\"row\">\r\n            {products.map((product) => (\r\n              <div key={product._id} className=\"col-md-4\">\r\n                <ProductCard product={product} />\r\n              </div>\r\n            ))}\r\n          </div>\r\n        )}\r\n      </div>\r\n\r\n      <div className=\"row\">\r\n        <nav className=\"col-md-4 offset-md-4 text-center pt-5 p-3\">\r\n          <Pagination\r\n            current={page}  //by default 1 \r\n            total={(productsCount / 3) * 10}  //total products by each(3) total of 10 pages will be displayed\r\n            onChange={(value) => setPage(value)}\r\n          />\r\n        </nav>\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default NewArrivals;\r\n","C:\\web development\\ebiblio\\client\\src\\components\\category\\categorylist.js",[],"C:\\web development\\ebiblio\\client\\src\\components\\home\\bestsellers.js",["343"],"import React, { useEffect, useState } from \"react\";\r\nimport { getProducts,getProductsCount } from \"../../functions/product\";\r\nimport ProductCard from \"../cards/productcard\";\r\nimport LoadingCard from \"../cards/loadi